{
  "name": "spiral/writeaway",
  "type": "library",
  "description": "PHP API server for Writeaway editor",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "spiral/helpers": "^1.0",
    "spiral/framework": "^3.0",
    "spiral/cycle-bridge": "^2.0",
    "doctrine/collections": "^1.6",
    "ext-imagick": "*",
    "spiral/validator": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "spiral/code-style": "^1.1",
    "spiral/nyholm-bridge": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\Writeaway\\": "src/",
      "Spiral\\": "spiral/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Tests\\Writeaway\\": "tests/"
    }
  }
}
