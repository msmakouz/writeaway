{
  "name": "spiral/writeaway",
  "type": "library",
  "description": "PHP API server for Writeaway editor",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "spiral/helpers": "^1.0",
    "spiral/framework": "^2.9",
    "spiral/cycle-bridge": "^1.0",
    "doctrine/collections": "^1.6",
    "ext-imagick": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "spiral/code-style": "^1.0",
    "spiral/roadrunner": "^1.9",
    "laminas/laminas-diactoros": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\Writeaway\\": "src/",
      "Spiral\\": "spiral/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Tests\\Writeaway\\": "tests/"
    }
  }
}
